{
  "name": "n8n-nodes-video-ai",
  "version": "0.4.4",
  "description": "n8n node for AI-powered video Operations (analysis, Review, Summarize, etc), currently supporting Google Gemini.",
  "keywords": [
    "n8n-community-node-package",
    "gemini",
    "video-analysis",
    "ai",
    "hadidiz",
    "hadidizflow.com",
    "hadidizflow",
    "ai Video",
    "video ai"
  ],
  "license": "MIT",
  "homepage": "https://HadidizFlow.com",
  "author": {
    "name": "Hadidiz",
    "email": "hadidizflow@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AhmedElhadidii/n8n-nodes-Video-AI.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GeminiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/HttpBin/HttpBin.node.js",
      "dist/nodes/VideoAiAnalysis/VideoAiAnalysis.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/node-fetch": "2",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.82.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "node-fetch": "2.7.0"
  }
}
