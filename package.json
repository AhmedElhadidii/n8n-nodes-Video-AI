{
  "name": "n8n-nodes-gemini-video",
  "version": "0.1.0",
  "description": "n8n node for video analysis using Google's Gemini AI",
  "keywords": [
    "n8n-community-node-package",
    "gemini",
    "video-analysis",
    "ai",
    "hadidiz Flow",
    "hadidiz",
    "hadidizFlow.com"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hadidiz/n8n-nodes-gemini-video",
  "author": {
    "name": "Hadidiz Flow",
    "email": "your-email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hadidiz/n8n-nodes-gemini-video.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ExampleCredentialsApi.credentials.js",
      "dist/credentials/HttpBinApi.credentials.js",
      "dist/credentials/GeminiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/HttpBin/HttpBin.node.js",
      "dist/nodes/GeminiVideo/GeminiVideo.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3",
    "n8n-workflow": "^1.82.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "node-fetch": "^3.3.2"
  }
}
